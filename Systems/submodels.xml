        <speed>700.0</speed>        <!-- ft/sec -->
        <repeat>true</repeat>
        <delay>0.25</delay>
        <count>40</count>
        <offsets>
            <x-m> 1.10</x-m>
            <y-m>-0.65</y-m>
            <z-m> 0.90</z-m>
            <pitch-deg>-90</pitch-deg>
