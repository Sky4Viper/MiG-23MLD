<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <checklist>
        <title>Engine Startup</title>  
        <item>
            <name>Battery</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/electric/switches/battery</property>
                    <value>1</value>
                </equals>
            </condition>
            <marker>
                <x-m>-4.874</x-m>
                <y-m> 0.306</y-m>
                <z-m>-0.217</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/electric/switches/battery</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>Booster Pump</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/electric/switches/pump</property>
                    <value>1</value>
                </equals>
            </condition>
            <marker>
                <x-m>-4.918</x-m>
                <y-m> 0.349</y-m>
                <z-m>-0.213</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/electric/switches/pump</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>APU</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/electric/switches/starterunit</property>
                    <value>1</value>
                </equals>
            </condition>
            <marker>
                <x-m>-4.918</x-m>
                <y-m> 0.328</y-m>
                <z-m>-0.213</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/electric/switches/starterunit</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>DC-Generator</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/electric/switches/dc-gen</property>
                    <value>1</value>
                </equals>
            </condition>
            <marker>
                <x-m>-4.874</x-m>
                <y-m> 0.330</y-m>
                <z-m>-0.217</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/electric/switches/dc-gen</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>AC-Generator</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/electric/switches/ac-gen</property>
                    <value>1</value>
                </equals>
            </condition>
            <marker>
                <x-m>-4.874</x-m>
                <y-m> 0.350</y-m>
                <z-m>-0.217</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/electric/switches/ac-gen</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>Engine Starter</name>
            <value>Activate</value>
            <condition>
                <greater-than>
                    <property>engines/engine/n1</property>
                    <value>5</value>
                </greater-than>
            </condition>
            <marker>
                <x-m>-4.872</x-m>
                <y-m>-0.374</y-m>
                <z-m> 0.057</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>nasal</command>
                <script>libraries.engineStart()</script>
            </binding>
        </item>
        <item>
            <name>Engine Cutoff</name>
            <value>OFF</value>
            <condition>
                <equals>
                    <property>controls/engines/engine[0]/cutoff</property>
                    <value>0</value>
                </equals>
            </condition>
            <marker>
                <x-m>-5.060</x-m>
                <y-m>-0.338</y-m>
                <z-m>-0.125</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>property-assign</command>
                <property>controls/engines/engine[0]/cutoff</property>
                <value>0</value>
            </binding>
        </item>
        <item>
            <name>APU</name>
            <value>OFF</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/electric/switches/starterunit</property>
                    <value>0</value>
                </equals>
            </condition>
            <marker>
                <x-m>-4.918</x-m>
                <y-m> 0.328</y-m>
                <z-m>-0.213</z-m>
                <scale>1.5000</scale>
            </marker>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/electric/switches/starterunit</property>
                <value>0</value>
            </binding>
        </item>
    </checklist>
</PropertyList>      
