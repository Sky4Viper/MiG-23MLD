<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <model>
        <name>gamma2</name>
        <path>Aircraft/MiG-23MLD/Models/Parts/seat.ac</path>
        <offsets>
            <x-m>0.3959</x-m>
            <y-m>0.0000</y-m>
            <z-m>1</z-m>
        </offsets>
    </model>
    <model>
        <name>gamma3</name>
        <path>Aircraft/MiG-23MLD/Models/Stores/ES/gamma2.xml</path>
    </model>
    <animation>
        <type>rotate</type>
        <object-name>gamma1</object-name>
        <object-name>gamma2</object-name>
        <object-name>gamma3</object-name>
        <offset-deg>90</offset-deg>
        <center>
            <x-m>0</x-m>
            <y-m>0.0000</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>select</type>
        <object-name>gamma2</object-name>
        <condition>
            <less-than>
                <property>payload/armament/es/flags/deploy-id-10</property>
                <value>0.99</value>
            </less-than>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>gamma3</object-name>
        <condition>
            <greater-than>
                <property>payload/armament/es/flags/deploy-id-10</property>
                <value>0.10</value>
            </greater-than>
        </condition>
    </animation>
    <animation>
        <type>scale</type>
        <object-name>gamma3</object-name>
        <property>payload/armament/es/flags/deploy-id-10</property>
        <z-factor>1.0</z-factor>
    </animation>
</PropertyList>
